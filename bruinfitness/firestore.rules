rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /schedules/San%20Leandro/schedule/{item} {
      allow read: if true;
    }
    match /schedules/Redwood%20City/dates/2020_12_13/classes/{item} {
    	allow read: if true;
      allow write: if true;
    }
    match /schedules/Redwood%20City/dates/2021-01-15/classes/{item} {
    	allow read: if true;
      allow write: if true;
    }
    match /workouts/{item} {
    	allow read: if request.auth != null;
      allow write: if request.auth.token.admin == true;
    }
  }
}